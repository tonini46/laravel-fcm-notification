{
  "name": "tonini46/laravel-fcm-notification",
  "description": "Laravel FCM (Firebase Cloud Messaging) Notification Channel",
  "keywords": [
    "laravel",
    "fcm",
    "firebase",
    "notifications"
  ],
  "license": "MIT",
  "authors": [
    {
        "name": "Swivernet",
        "email": "swivernet@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3|^8.0",
    "laravel/framework": "^5.7|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "guzzlehttp/guzzle": "*",
    "illuminate/notifications": "*",
    "illuminate/queue": "*",
    "illuminate/support": "*",
    "illuminate/config": "*"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Tonini46\\FCM\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tonini46\\FCM\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tonini46\\FCM\\FcmNotificationServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
